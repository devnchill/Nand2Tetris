/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0],b=in[1],out=g1);
    Or(a=g1,b=in[2],out=g2);
    Or(a=g2,b=in[3],out=g3);
    Or(a=g3,b=in[4],out=g4);
    Or(a=g4,b=in[5],out=g5);
    Or(a=g5,b=in[6],out=g6);
    Or(a=g6,b=in[7],out=g7);
    Or(a=g7,b=in[8],out=out);
}
